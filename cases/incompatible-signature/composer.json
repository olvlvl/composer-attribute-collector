{
  "require": {
    "php": ">=8.0",
    "psr/log": "^3.0",
    "olvlvl/composer-attribute-collector": "^2.1.0"
  },
  "config": {
    "allow-plugins": {
      "olvlvl/composer-attribute-collector": true
    }
  },
  "extra": {
    "composer-attribute-collector": {
      "include": [
        "src"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Acme\\": "src"
    }
  }
}
